{
	"name": "wp-dev",
	"dockerComposeFile": "docker-compose.yaml",
	"service": "wordpress",
	"workspaceFolder": "/",
	"shutdownAction": "stopCompose",
	"customizations": {
		"vscode": {
			"settings": {
				"php.validate.executablePath": "/usr/local/bin/php"
			},
			"extensions": [
				"eamodio.gitlens"
			]
		}
	},
	"postCreateCommand": "sh ./.devcontainer/dev-build.sh",
	"postAttachCommand": "sh ./.devcontainer/dev-embed.sh"
}